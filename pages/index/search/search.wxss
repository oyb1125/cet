.container{
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100vh;
  background-color: #f9f9f9
}
.search_row{
  width: 100%;
  display: flex;
  flex-direction: row;
  margin: 10rpx;
  justify-content: space-around;
}
input{
  width: 70%;
  border-radius: 20rpx;
  height: 70rpx;
  background: #ececec;
}
/* 推荐搜索样式 */
.history {
  width: 80vw;
  margin: 3vw 0 0 8vw;
}
.historyList {
  display: inline-block;
  padding: 40rpx 40rpx;
  background: #ffffff;
  margin: 3vw 5vw 0 0;
  box-shadow:0rpx 4rpx 8rpx 4rpx #DDDDDD;
  border-radius: 10rpx
}
/* 搜索结果样式 */
.text-view {
  font-size: 1.4rem;
}
.content {
  width: 72vw;
  padding: 40rpx 60rpx;
  margin: 40rpx auto;
  background: #ffffff;
  box-shadow:0rpx 4rpx 8rpx 4rpx #DDDDDD;
  border-radius: 10rpx
}
.word-content {
  display: flex;
  justify-content: flex-start;
  margin: 20rpx 0;
}
.sound {
  width: 6vw;
}
.word-pct {
  line-height: 6vw;
  margin: 0 3vw;
  color: #818181;
}
.defin {
  line-height: 58rpx;
}
.noContent {
  text-align: center;
  margin: 5vw 0;
  color: #818181;
}