.container{
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #f9f9f9;
}
.cle{
  display: flex;
  flex-direction: row;
  width: 90%;
}
.box{
  display: flex;
  flex-direction: column;
  padding: 40rpx 40rpx;
  background: #ffffff;
  width: 80%;
  margin: 10rpx auto;
  box-shadow:0rpx 4rpx 8rpx 4rpx #DDDDDD;
  border-radius: 10rpx
}
.item{
  display: flex;
  flex-direction: row;
}
.audio{
  display: flex;
  flex-direction: row;
}
image{
  width: 50rpx;
  height: 50rpx;
}